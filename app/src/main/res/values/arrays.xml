    <!-- The array below defines which leagues we want to fetch/present to the user:
        394- 1. Bundesliga
        395- 2. Bundesliga
        398- Premier League
        399- Primera Division
        405- Champions League
        NOTE: a linear search is used, so these numbers can be in any order
      -->
    <array name="leagues_to_fetch">
        <item>394</item>
        <item>395</item>
        <item>398</item>
        <item>399</item>
        <item>405</item>
    </array>

    <!-- This array is used to configure which leagues are "champion leagues" and presented with
         different match day names based on what day of the championship match is being played.
         For Fall 2015, this league appears to be "405" based on current matches seen on television
      -->
    <array name="league_champions">
        <item>405</item>
    </array>

    <!-- The two tables below define all leagues recognized by the app with the league number and
         corresponding name in the same relative positions in their respective arrays.
         NOTE: this list needs to be updated in 2016
      -->
